#!/usr/bin/env python
# -*- coding: us-ascii -*-
# vim: syntax=python

import sys
from bglib.doc.bgwiki import Formatter
from bglib.doc.mock import DataBaseMock

db = DataBaseMock()
formatter = Formatter(db)
text = sys.stdin.read().decode('shift-jis')
formatter.parse(text)
print formatter.make_html().encode('utf-8')
  

